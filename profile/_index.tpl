<main class="cpn-default">
	<h1 class="cpn-header">
		<span style="float:right" class="small">[profile.count;magnet=span] Users</span>
		[af.title;ifempty='User Profiles']
	</h1>
	<div class="cpn-default">
		<div class="cpn-profile-list-type">
			<a href="[afurl.base]/profile/map" style="width:956px">User Profile World Map</a>
		</div>
		<div class="cpn-profile-list-letter">
			<a href="[afurl.base]/profile/[letters.$]">[letters.val;block=a]</a>
		</div>
		<div class="cpn-profile-list-type">
			<a href="[afurl.base]/profile/[types.$]">[types.val;block=a]</a><a href="[afurl.base]/profile/new">Newest Profiles</a><a href="[afurl.base]/profile/random">Random Profile</a>
		</div>
	</div>
</main>
