<article class="cpn-default">
	<h1 class="cpn-header">
		<span class="smaller" style="float:right">Total: [team.#]</span>
		Teams
	</h1>

	<div class="cpn-default">

		<ul class="cpn-account-list">
			<li>
				<a class="cpn-account-name" href="[afurl.base]/team/[team.user_url;ifempty='[team.user_id]';block=li]" data-id="[team.user_id]">
					<img src="[team.img;ifempty='[afurl.static]/thumb2/profile.svg']" alt="[team.user_name]" />
					[team.user_name]
					<span class="cpn-account-subtext">[team.user_tagline]</span>
				</a>
			</li>
		</ul>

	</div>
</article>
