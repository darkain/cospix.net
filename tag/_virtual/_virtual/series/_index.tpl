<section style="margin-left:-13px">


	<div class="profile-box">
		<h3 class="cpn-header">[af.title]</h3>
		<div class="profile-box-body" style="padding:2px">
			<ul class="cpn-tag-boxes">
				<li itemprop="character" itemscope itemtype="http://schema.org/Person">
					<a href="[afurl.base]/tag/[series.group_type_name]/[series.group_label;f=urlname;block=li;bmagnet=((div))]" class="cpn-thumb-link" itemprop="url"><figure>
						<img src="[series.img;noerr;ifempty='[afurl.static]/thumb2/character.svg']" itemprop="image" />
						<figcaption itemprop="name">[series.group_label]</figcaption>
					</figure></a>
				</li>
			</ul>
			<div class="clear"></div>
		</div>
	</div>


</section>
