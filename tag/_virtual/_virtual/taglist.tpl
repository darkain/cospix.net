<article class="cpn-default cpn-discover cpn-discover-no-icon">
	<h1 class="cpn-header">[af.title]</h1>

	[onload;file=tag/list.tpl]

	<div class="cpn-default" style="background:none">
		<ul class="cpn-map-list cpn-tag-list">
			<li><a href="[afurl.base]/tag/[nttags.group_type_name]/[nttags.group_label;f=urlname;block=li]">
				<img src="[afurl.static]/thumb2/[nttags.group_type_name;ifempty=blank].svg" alt="[series.group_label]" />
				<strong>[nttags.group_label]</strong>
			</a></li>
		</ul>
		<div class="clear"></div>
	</div>
</article>


[onload;file=../type.tpl]
