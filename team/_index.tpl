<main class="cpn-default">
	<h1 class="cpn-header">[af.title]</h1>
	<div class="cpn-default">

		<ul class="cpn-account-list">
			<li>
				<a class="cpn-account-name" href="[afurl.base]/team/[team.user_url;ifempty='[team.user_id]';block=li]">
					<img src="[team.img;ifempty='[afurl.static]/thumb2/profile.svg']" alt="[team.user_name]" />
					[team.user_name]
					<span class="cpn-account-subtext">[team.user_tagline]</span>
					<span class="cpn-account-type">[team.types;noerr;magnet=span]</span>
				</a>
			</li>
		</ul>

	</div>
</main>
