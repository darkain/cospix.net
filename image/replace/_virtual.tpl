<article class="cpn-default">
	<h1 class="cpn-header">Replace Image</h1>
	<section class="cpn-default">
		<img src="[image.img]" /><br />

		<form action="[afurl.base]/image/replace/save" method="GET"><div style="margin:1em">
			<input type="hidden" name="old" value="[image.hash]" />
			<input type="text" name="new" style="width:20em" />
			<input type="submit" value="Change" id="btnhash" />
		</div></div>

	</section>
</article>

